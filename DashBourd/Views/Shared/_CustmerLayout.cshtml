<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Movies Website</title>
	<link rel="stylesheet" href="style.css">
	<link rel="stylesheet" href="~/css/_CustmerLayout.css" />
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.css" />
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>

	<section>

		<nav>

			<div class="logo">
				<h1>Cema<span>Pure</span></h1>
			</div>

			<div class="right">

				<ul>
					<li><a href="#">Home</a></li>
					<li><a href="#">About</a></li>
					<li><a href="#">TV Show</a></li>
					<li><a href="#">Newsletter</a></li>
					<li><a href="#">Blog</a></li>
					<li><a href="#">Contact</a></li>
				</ul>

			</div>

		</nav>

		<div class="container">
			<main role="main" class="pb-3">
				@RenderBody()
			</main>
		</div>
	</section>

	<div class="newsletter">

		<h1>Newsletter</h1>

		<div class="news_card">

			<input type="email" name="" id="" placeholder="Enter your email">
			<button type="submit">Subscribe</button>

		</div>

	</div>

		<!--Dock -->
	<div class="Dock">
		<div class="dock">
			<a href="/" class="dock-item" data-tooltip="Home">
				<svg class="icon" viewBox="0 0 24 24" fill="none" stroke-width="2">
					<path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z" />
					<polyline points="9 22 9 12 15 12 15 22" />
				</svg>
			</a>
			<a href="#magic-dock" class="dock-item" data-tooltip="Docs">
				<svg class="icon" viewBox="0 0 24 24" fill="none" stroke-width="2">
					<path d="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z" />
					<polyline points="14 2 14 8 20 8" />
				</svg>
			</a>
			<a href="#installation" class="dock-item" data-tooltip="Install">
				<svg class="icon" viewBox="0 0 24 24" fill="none" stroke-width="2">
					<path d="M12 5v14" />
					<polyline points="19 12 12 19 5 12" />
				</svg>
			</a>
			<a href="#usage" class="dock-item" data-tooltip="Usage">
				<svg class="icon" viewBox="0 0 24 24" fill="none" stroke-width="2">
					<path d="M16 18a4 4 0 0 0-8 0" />
					<circle cx="12" cy="12" r="10" />
					<line x1="12" y1="2" x2="12" y2="6" />
					<line x1="12" y1="18" x2="12" y2="22" />
				</svg>
			</a>
			<a href="#api-reference" class="dock-item" data-tooltip="API">
				<svg class="icon" viewBox="0 0 24 24" fill="none" stroke-width="2">
					<path d="M4 19h16M4 5h16M4 12h16" />
				</svg>
			</a>
		</div>
	</div>
	















	<script src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>

	<!-- Initialize Swiper -->
	<script>
		var swiper = new Swiper(".mySwiper", {
		  slidesPerView: 1,
		  spaceBetween: 30,
		  loop: true,
		  pagination: {
			el: ".swiper-pagination",
			clickable: true,
		  },
		  navigation: {
			nextEl: ".swiper-button-next",
			prevEl: ".swiper-button-prev",
		  },
		});
	</script>

</body>
</html>